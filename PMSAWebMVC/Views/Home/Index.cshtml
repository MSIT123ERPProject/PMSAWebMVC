@{
    ViewBag.Title = "首頁";
}


@*供應商倉管人員首頁圖表*@
@if (User.IsInRole("Supplier"))
{
    <script>
        document.body.style.backgroundColor = "white";
    </script>
    <div class="jumbotron" style="background-image: url(data:image/jpeg;base64,);">
        <h1 class="text-light">@PMSAWebMVC.Resources.AppResource.chokobo</h1>
        <p class="lead text-light">最棒的腳踏車品牌。</p>
        <a href="https://asp.net" class="btn btn-primary btn-lg"><h6 class="m-0">最新消息 &raquo;</h6></a>
    </div>
    <div class="row">
        <div class="col-md-4">
            @*<h2><i class="fas fa-money-check-alt"></i>出貨金額比例</h2>*@
            @*DateTimePickerStart*@
            <div class="input-group date" id="datetimepickerIdStart" data-target-input="nearest">
                <div class="input-group-text">開始</div>
                <input type="text" class="form-control datetimepicker-input" data-target="#datetimepickerIdStart" data-toggle="datetimepicker" />
                <div class="input-group-append" data-target="#datetimepickerIdStart" data-toggle="datetimepicker">
                    <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                </div>
            </div>
            @*DateTimePickerEnd*@
            <div class="input-group date" id="datetimepickerIdEnd" data-target-input="nearest">
                <div class="input-group-text">結束</div>
                <input type="text" class="form-control datetimepicker-input" data-target="#datetimepickerIdEnd" data-toggle="datetimepicker" />
                <div class="input-group-append" data-target="#datetimepickerIdEnd" data-toggle="datetimepicker">
                    <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                </div>
            </div>
            <div id="container"></div>
        </div>
        <div class="col-md-4">
            @*<h2><i class="fas fa-chart-bar"></i>料件庫存數量</h2>*@
            <div id="container2"></div>
        </div>
        <div class="col-md-4">
            @*<h2><i class="fas fa-boxes"></i>料件出貨數量</h2>*@
            <div id="container3"></div>
        </div>
    </div>
    @section scripts{
        @*bubbleChart還是需要引用此CDN*@
        <script src="https://code.highcharts.com/highcharts-more.js"></script>
        @*//////////////////////////////////////////*@
        @*引用highchart主題*@
        <script src="~/scripts/highcharts/themes/sunset.js"></script>
        @*//////////////////////////////////////////*@
        <script>
            $(document).ready(function () {
                unitsInStockChart();
                partTotalPrice();
                SplitBubbleChart();
            });
            //時間選擇器
            $('#datetimepickerIdStart').datetimepicker(
                {
                    defaultDate: new Date("2019/12/01"),
                }
            );
            $('#datetimepickerIdEnd').datetimepicker(
                {
                    defaultDate: new Date(),
                }
            );
            let dateTimeStart ;
            let dateTimeEnd ;
            $('#datetimepickerIdStart').on('change.datetimepicker', function (e) {
                //console.log(e);
                //console.log(this);
                dateTimeStart = $(this).find("input").val();
            });
            $('#datetimepickerIdEnd').on('change.datetimepicker', function (e) {
                dateTimeEnd = $(this).find("input").val();
                //檢查是否日期選擇正確
                if (dateTimeStart > dateTimeEnd) {
                    toastr.error("起始日期不得小於結束日期", "警告");
                }
                else {
                    partTotalPrice();
                }
            });
            //設定HIGHCHART公用設定
            Highcharts.setOptions({
                chart: {
                    style: {
                        fontWeight: 'bold',
                        fontFamily: "微軟正黑體",
                        fontsize: "30px",
                    },
                    backgroundColor: '#A5DEE4',
                    width: 510,
                },
                title: {
                    useHTML: true,
                    style: {
                        fontWeight: 'bold',
                        fontFamily: "微軟正黑體",
                        fontSize: "30px"
                    },
                },
                xAxis: {
                    labels: {
                        style: {
                            fontWeight: 'bold',
                            fontFamily: "微軟正黑體",
                            fontSize: "15px"
                        },
                    }
                },
                yAxis: {
                    title: {
                        style: {
                            fontWeight: 'bold',
                            fontFamily: "微軟正黑體",
                            fontSize: "18px"
                        },
                    },
                    labels: {
                        style: {
                            fontWeight: 'bold',
                            fontFamily: "微軟正黑體",
                            fontSize: "15px"
                        },
                    }
                },
                plotOptions: {
                    series: {
                        dataLabels: {
                            style: {
                                fontWeight: 'bold',
                                fontFamily: "微軟正黑體",
                                fontSize: "13px"
                            },
                        }
                    },
                },
                tooltip: {
                    style: {
                        fontWeight: 'bold',
                        fontFamily: "微軟正黑體",
                        fontSize: "15px"
                    },
                },
                legend: {
                    style: {
                        fontWeight: 'bold',
                        fontFamily: "微軟正黑體",
                        fontSize: "20px"
                    }
                },

            });


            ////////////////////////////////////
        //庫存不足料件圖 column chart
        function unitsInStockChart() {
               $.getJSON(`@Url.Action("GetStockData", "SupplierHomePage")`, function (datas) {
                   let safetyQty = [];
                   let partNumber = [];
                   let unitsInStock = [];
                   let unitsOnOrder = [];
                for (let i = 0; i < datas.length; i++) {
                    partNumber.push(datas[i].PartNumber);
                    safetyQty.push(datas[i].SafetyQty);
                    unitsInStock.push(datas[i].UnitsInStock);
                    unitsOnOrder.push(datas[i].UnitsOnOrder);
                };
                var chart2 = new Highcharts.chart({
                    title: {
                        useHTML: true,
                        text: '<i class="fas fa - chart - bar"></i>目前庫存量' ,
                    },
                    subtitle: { text: '料件' },
                    chart: {
                        renderTo: 'container2',
                        type: 'column',
                        height:510,
                    },
                    tooltip: {
                        headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
                        pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
                            '<td style="padding:0"><b>{point.y:.1f} EA</b></td></tr>',
                        footerFormat: '</table>',
                        shared: true,
                        useHTML: true
                    },
                    xAxis: {
                        categories: partNumber
                    },
                    yAxis: [{
                        min: 0,
                        title: {
                            text: '庫存',
                            align:'high'
                        },
                        labels: {
                            overflow:'justify'
                        }
                    },
                        {
                            title: 'safetyQty',
                            opposite: true
                        }
                    ],
                    series: [
                        {
                        name:'庫存',
                        data: unitsInStock
                    },
                        {
                            name: '安全庫存量',
                            data: safetyQty
                        },
                        {
                            name: '訂單數量',
                            data: unitsOnOrder
                        }
                    ],
                    plotOptions: {
                        column: {
                            pointPadding: 0.2,
                            borderWidth: 0
                        }
                    },
                    legend: {
                        layout: 'vertical',
                        align: 'right',
                        vertical: '',
                        x: -40,
                        y: 80,
                        floating: true,
                        borderwidth: 1,
                        backgroundColor: Highcharts.defaultOptions.legend.backgroundColor || '#FFFFFF',
                        shadow: true
                    },
                    credits: {
                        enable: false
                    }
                });
            });
        }
        //三個月內各料件出貨金額 PIE CHART
            function partTotalPrice() {
                //先把日期分別放進物件中
                let data = {
                    dateStart : dateTimeStart,
                    dateEnd: dateTimeEnd
                };
                $.getJSON(`@Url.Action("GetPartTotalPricePercentage", "SupplierHomePage")`, data, function (datas) {
                let percentage = [];
                let partname = [];
                let pie = [];
                for (var i = 0; i < datas.length; i++) {
                    percentage.push(datas[i].Percentage);
                    partname.push(datas[i].PartName);
                    pie.push({ name: datas[i].SourceListID, y: datas[i].ToalPrice });
                    }
                    if (pie[0] == null) {
                        toastr.error("查無資料", "通知");
                    }
                    var chart = new Highcharts.chart({
                        chart: {
                            renderTo: 'container',
                            plotBackgroundColor: null,
                            plotBorderWidth: null,
                            plotShadow: false,
                            type: 'pie',
                            height:434,
                        },
                        title: {
                            text: '各料件已出貨金額百分比',
                        },
                        tooltip: {
                            pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
                        },
                        plotOptions: {
                            pie: {
                                allowPointSelect: true,
                                cursor: 'pointer',
                                dataLabels: {
                                    enabled: true,
                                    format: '<b>{point.name}</b>: {point.percentage:.1f} %'
                                },
                                showInLegend: true,
                            },
                        },
                    });
                    chart.addSeries({
                        type: 'pie',
                        name: '商品金額比例',
                        data: pie
                    }, false);
                    chart.redraw();
            });
        }
        //BUBBLECHART
        function SplitBubbleChart() {
            $.getJSON(`@Url.Action("GetPartQtyByShipStatus", "SupplierHomePage")`, function (datas) {
                let chart3 = new Highcharts.chart('container3',{
                    chart: {
                       type: 'packedbubble',
                        height:'100%'
                    },
                    title: {
                        text:'出貨數量'
                    },
                    tooltip: {
                        useHTML: true,
                        pointFormat: '<b>{point.name}:</b> {point.value}個<sub></sub>'
                    },
                    plotOptions: {
                        packedbubble: {
                            minSize: '30%',
                            maxSize: '120%',
                            zMin:0,
                            zMax:1000,
                            layoutAlgorithm: {
                                splitSeries: false,
                                gravitationalConstant:  0.02
                            },
                            dataLabels: {
                                enabled: true,
                                format: '{point.name}',
                                filter: {
                                    property: 'y',
                                    operator: '>',
                                    value:250
                                },
                                style: {
                                    color: 'black',
                                    textOutline: 'none',
                                    fontWeight:'normal'
                                }
                            }
                        }
                    },
                    series: datas
                });
            });
        }
        </script>
    }

}
