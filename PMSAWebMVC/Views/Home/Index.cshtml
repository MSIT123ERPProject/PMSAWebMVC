                    var sss = [];
                    for (var i = 0; i < data.length; i++) {
                        Names.push(data[i].name);
                        sss.push(data[i].count);
                    }

                    Highcharts.chart('sss', {
                        chart: {
                            type: 'column'
                        },
                        title: {
                            text: '進貨單處於簽核中'
                        },
                        xAxis: {
                            categories: Names,
                            crosshair: true
                        },
                        yAxis: {
                            min: 0,
